{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@interfaces/*": ["./src/interfaces/*"],
      "@utils/*": ["./src/utils/*"],
      "@images/*": ["./src/assets/images/*"],
      "@info/*": ["./src/info/*"],
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}